@model DaPlatform.Models.ImagePullFormViewModel
@{
    ViewBag.Title = "ImagePull";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ImagePull</h2>

@using (Html.BeginForm("Pull", "Images"))
{
    @Html.ValidationSummary(true, "Please fix the following errors.")
    <div class="form-group">
        @Html.LabelFor(m => m.imageID)
        @Html.TextBoxFor(m => m.imageID, new { @class = "form-control" , @readonly = "readonly" })
        @Html.ValidationMessageFor(m => m.imageID)
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.userPrivateKey)
        @Html.TextAreaFor(m => m.userPrivateKey, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.userPrivateKey)
    </div>
    @Html.AntiForgeryToken()
    <button type="submit" class="btn btn-primary">Pull</button>
}


@section scripts
{
    @Scripts.Render("~/bundles/jqueryval")
}
